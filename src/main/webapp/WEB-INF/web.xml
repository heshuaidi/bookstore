<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
    <servlet>
        <servlet-name>ManageServlet</servlet-name>
        <servlet-class>com.shuaidi.web.controller.ManageServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ClientServlet</servlet-name>
        <servlet-class>com.shuaidi.web.controller.ClientServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>ManageServlet</servlet-name>
        <url-pattern>/manage/ManageServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ClientServlet</servlet-name>
        <url-pattern>/client/ClientServlet</url-pattern>
    </servlet-mapping>
</web-app>
